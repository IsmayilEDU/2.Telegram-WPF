    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icons\attach.png" />
    <None Remove="icons\dots.png" />
    <None Remove="icons\menu.png" />
    <None Remove="icons\microphone-black-shape.png" />
    <None Remove="icons\search.png" />
